{
	"name": "rvk-styles",
	"version": "0.2.5",
	"main": "index.js",
	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "NODE_ENV=production  yarn run cssserve --config cssserve-prod.json",
		"dev:server": "NODE_ENV=development  yarn run cssserve --config scripts/cssserve-dev.json",
		"publish:css": "NODE_ENV=production yarn run gulp publishCss",
		"publish:devcss": "NODE_ENV=debug  yarn run gulp publishDevCss",
		"publish:assets": "NODE_ENV=production  yarn run gulp publishAssets",
		"publish:all": "yarn run publish:assets  &&  yarn run publish:css  &&  yarn run publish:devcss",
		"dev": "yarn run gulp watch"
	},
	"engines": {
		"node": "^12"
	},
	"dependencies": {
		"cssserve": "^1.1.2"
	},
	"devDependencies": {
		"@hugsmidjan/gulp-iconfont": "^0.3.2",
		"@hugsmidjan/gulp-images": "^0.1.5",
		"@hugsmidjan/gulp-sass": "^0.1.2",
		"@hugsmidjan/hxmstyle": "^2.4.0",
		"del": "^5.1.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-destructuring": "^2.1.0",
		"eslint-plugin-prettier": "^3.1.2",
		"gulp": "^4.0.2",
		"md5-file": "^4.0.0",
		"onchange": "^6.1.0",
		"ospec": "^4.0.1",
		"prettier": "^1.19.1"
	},
	"browserslist": [
		"defaults"
	],
	"hxmstyle": {
		"options": {},
		"dependenciesAdded": [
			"eslint",
			"eslint-config-prettier",
			"eslint-plugin-destructuring",
			"eslint-plugin-prettier",
			"prettier"
		]
	},
	"author": "Hugsmiðjan ehf (http://www.hugsmidjan.is)",
	"contributors": [
		"Kári Yngvason <kari@hugsmidjan.is>",
		"Már Örlygsson <mar@hugsmidjan.is>",
		"Valur Sverrisson <valur@hugsmidjan.is>"
	],
	"repository": "ssh://git@stash.hugsmidjan.is:7999/proj/rvk-styles.git"
}
